{
  "name": "@authereum/starkware-crypto",
  "description": "Starkware Crypto Library",
  "version": "1.9.7-beta.2",
  "author": "Pedro Gomes <github.com/pedrouid>",
  "license": "Apache-2.0",
  "keywords": [
    "stark",
    "starkware",
    "crypto",
    "elliptic",
    "curve",
    "cryptography"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "unpkg": "dist/umd/index.min.js",
  "homepage": "https://github.com/pedrouid/starkware-crypto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrouid/starkware-crypto.git"
  },
  "bugs": {
    "url": "https://github.com/pedrouid/starkware-crypto/issues"
  },
  "scripts": {
    "start": "tsdx watch",
    "clean": "rm -rf dist",
    "build:cjs": "./node_modules/.bin/tsc -p tsconfig.cjs.json",
    "build:umd": "webpack",
    "build": "yarn clean && yarn build:cjs && yarn build:umd",
    "test": "tsdx test ./test",
    "lint": "prettier-standard --format",
    "prepare": "yarn lint && yarn build",
    "publish:beta": "npm publish --tag beta --access public",
    "publish:latest": "npm publish --tag beta --access public"
  },
  "dependencies": {
    "bip39": "^3.0.2",
    "bn.js": "^5.1.2",
    "elliptic": "^6.5.3",
    "enc-utils": "^2.2.2",
    "ethereumjs-wallet": "^1.0.0",
    "hash.js": "^1.1.7",
    "js-sha3": "^0.8.0",
    "keccak256": "^1.0.2",
    "rsv-signature": "^1.1.0",
    "@authereum/starkware-types": "1.2.0-beta.1",
    "web3-utils": "^1.3.0"
  },
  "devDependencies": {
    "@types/elliptic": "6.4.12",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "husky": "4.2.5",
    "prettier-standard": "^16.4.1",
    "tsdx": "0.13.2",
    "tslib": "1.10.0",
    "typescript": "3.7.5",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
